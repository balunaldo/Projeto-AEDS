{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compilar Teste (testar.c)",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-g",
        "TConj.c",
        "testar.c",
        "-o",
        "output\\testar.exe",
        "-mconsole"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      }
    }
  ]
}